{
  "name": "Productivity Pal",
  "description": "A helpful little tool for staying on task.",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "Backend/background.js"
  },
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "Frontend/popup/popup.html",
    "default_icon": {
      "16": "Frontend/icons/get_started16.png",
      "32": "Frontend/icons/get_started32.png",
      "48": "Frontend/icons/get_started48.png",
      "128": "Frontend/icons/get_started128.png"
    }
  },
  "icons": {
    "16": "Frontend/icons/get_started16.png",
    "32": "Frontend/icons/get_started32.png",
    "48": "Frontend/icons/get_started48.png",
    "128": "Frontend/icons/get_started128.png"
  }
}
